                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH RankedStatus AS (
  SELECT
    [order_id],
    [status_id],
    [status_date],
    ROW_NUMBER() OVER (PARTITION BY [order_id], [status_id] ORDER BY [status_date] DESC) AS rn
  FROM
    order_history oh1

)
SELECT
  [order_id],
  MAX(CASE WHEN [status_id] = 1 AND rn = 1 THEN [status_date] END) AS 'Order Received',
  MAX(CASE WHEN [status_id] = 2 AND rn = 1 THEN [status_date] END) AS 'Pending Delivery',
  MAX(CASE WHEN [status_id] = 3 AND rn = 1 THEN [status_date] END) AS 'Delivery In Progress',
  MAX(CASE WHEN [status_id] = 4 AND rn = 1 THEN [status_date] END) AS 'Delivered',
  MAX(CASE WHEN [status_id] = 5 AND rn = 1 THEN [status_date] END) AS 'Cancelled',
  MAX(CASE WHEN [status_id] = 6 AND rn = 1 THEN [status_date] END) AS 'Returned',
  MAX(CASE WHEN rn = 1 THEN [status_id] END) AS 'Current Status'
FROM
  RankedStatus
GROUP BY
  [order_id];</property>
